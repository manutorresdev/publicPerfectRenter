{
  "plugins": ["jsdoc-http-plugin"],
  "source": {
    "include": ["./"],
    "excludePattern": "(node_modules|jsdocEXAMPLE.js)"
  },
  "templates": {
    "cleverlinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "recurse": "true",
    "destination": "./docs",
    "template": "node_modules/docdash",
    "readme": "./READMEJSDOC.MD"
  }
}
